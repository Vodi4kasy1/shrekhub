local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Shrek Hub", "Midnight")
local Tab = Window:NewTab("Shrek Horror 2")
local Section = Tab:NewSection("Scripts")
Section:NewButton("Shrek Horror 2", "ButtonInfo", function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/Vodi4kasy1/shrekhub/main/Idk.lua"))()
end)
local Tab = Window:NewTab("YBA")
local Section = Tab:NewSection("Scripts")
Section:NewButton("Yba Shrek Hub", "ButtonInfo", function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/Vodi4kasy1/shrekhub/main/Shrek.lua"))()
end)
Section:NewButton("Yba BeachBoy v3", "ButtonInfo", function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/zakater5/LuaRepo/main/YBA/v3.lua"))()
end)
